{
    "name": "integration-service",
    "version": "1.0.0",
    "description": "Health data integration service for Holistic Health OS",
    "main": "dist/index.js",
    "scripts": {
      "start": "node dist/index.js",
      "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
      "build": "tsc -p tsconfig.json",
      "clean": "rimraf dist",
      "lint": "eslint src/**/*.ts",
      "lint:fix": "eslint src/**/*.ts --fix",
      "format": "prettier --write \"src/**/*.ts\"",
      "test": "jest",
      "test:coverage": "jest --coverage",
      "test:watch": "jest --watch",
      "prepare": "npm run build",
      "typecheck": "tsc --noEmit"
    },
    "engines": {
      "node": ">=18.0.0"
    },
    "keywords": [
      "integration",
      "health",
      "microservice",
      "fitbit",
      "garmin",
      "apple-health",
      "google-fit"
    ],
    "author": "Holistic Health OS Team",
    "license": "MIT",
    "dependencies": {
      "@types/express": "^5.0.1",
      "@types/node": "^22.14.0",
      "axios": "^1.6.8",
      "cors": "^2.8.5",
      "crypto-js": "^4.2.0",
      "dotenv": "^16.4.7",
      "express": "^5.1.0",
      "express-rate-limit": "^7.5.0",
      "helmet": "^8.1.0",
      "joi": "^17.13.3",
      "jsonwebtoken": "^9.0.2",
      "morgan": "^1.10.0",
      "node-cache": "^5.1.2",
      "oauth-1.0a": "^2.2.6",
      "pg": "^8.14.1",
      "ts-node": "^10.9.2",
      "ts-node-dev": "^2.0.0",
      "uuid": "^11.1.0",
      "winston": "^3.13.0"
    },
    "devDependencies": {
      "@types/cors": "^2.8.17",
      "@types/crypto-js": "^4.2.2",
      "@types/jest": "^29.5.12",
      "@types/jsonwebtoken": "^9.0.9",
      "@types/morgan": "^1.9.9",
      "@types/pg": "^8.11.11",
      "@types/supertest": "^6.0.3",
      "@types/uuid": "^9.0.8",
      "@typescript-eslint/eslint-plugin": "^7.14.0",
      "@typescript-eslint/parser": "^7.14.0",
      "eslint": "^8.57.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-prettier": "^5.1.3",
      "jest": "^29.7.0",
      "nodemon": "^3.1.9",
      "prettier": "^3.3.0",
      "rimraf": "^5.0.5",
      "supertest": "^6.3.4",
      "ts-jest": "^29.1.2",
      "typescript": "^5.8.3"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/holistic-health-os/integration-service.git"
    },
    "bugs": {
      "url": "https://github.com/holistic-health-os/integration-service/issues"
    },
    "homepage": "https://github.com/holistic-health-os/integration-service#readme"
  }