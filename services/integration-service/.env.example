# Node environment
NODE_ENV=development
PORT=3003
API_PREFIX=/api/v1
CORS_ORIGINS=http://localhost:3000,http://localhost:3001,http://localhost:3002

# Authentication
JWT_SECRET=your_jwt_secret_key_here

# Database configuration
DB_HOST=localhost
DB_PORT=5432
DB_DATABASE=holistic_health_os
DB_USER=postgres
DB_PASSWORD=postgres
DB_SSL=false
DB_POOL_MAX=20
DB_IDLE_TIMEOUT=30000
DB_CONNECTION_TIMEOUT=2000

# Token Encryption
TOKEN_ENCRYPTION_KEY=your_token_encryption_key_here

# Logging
LOG_LEVEL=info
LOG_DIR=logs

# Apple Health Provider
APPLE_HEALTH_CLIENT_ID=your_apple_client_id
APPLE_HEALTH_TEAM_ID=your_apple_team_id
APPLE_HEALTH_KEY_ID=your_apple_key_id
APPLE_HEALTH_PRIVATE_KEY=your_apple_private_key
APPLE_HEALTH_REDIRECT_URI=http://localhost:3003/api/v1/connection/apple-health/callback

# Google Fit Provider
GOOGLE_FIT_CLIENT_ID=your_google_client_id
GOOGLE_FIT_CLIENT_SECRET=your_google_client_secret
GOOGLE_FIT_REDIRECT_URI=http://localhost:3003/api/v1/connection/google-fit/callback

# Fitbit Provider
FITBIT_CLIENT_ID=your_fitbit_client_id
FITBIT_CLIENT_SECRET=your_fitbit_client_secret
FITBIT_REDIRECT_URI=http://localhost:3003/api/v1/connection/fitbit/callback

# Garmin Provider
GARMIN_CONSUMER_KEY=your_garmin_consumer_key
GARMIN_CONSUMER_SECRET=your_garmin_consumer_secret
GARMIN_REDIRECT_URI=http://localhost:3003/api/v1/connection/garmin/callback

# Samsung Health Provider
SAMSUNG_HEALTH_CLIENT_ID=your_samsung_health_client_id
SAMSUNG_HEALTH_CLIENT_SECRET=your_samsung_health_client_secret
SAMSUNG_HEALTH_REDIRECT_URI=http://localhost:3003/api/v1/connection/samsung-health/callback

# Withings Provider
WITHINGS_CLIENT_ID=your_withings_client_id
WITHINGS_CLIENT_SECRET=your_withings_client_secret
WITHINGS_REDIRECT_URI=http://localhost:3003/api/v1/connection/withings/callback