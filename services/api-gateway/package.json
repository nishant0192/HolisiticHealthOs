{
    "name": "api-gateway",
    "version": "1.0.0",
    "description": "API Gateway for Holistic Health OS",
    "main": "dist/index.js",
    "scripts": {
      "start": "node dist/index.js",
      "dev": "nodemon --exec ts-node src/index.ts",
      "build": "tsc",
      "clean": "rimraf dist",
      "lint": "eslint src/**/*.ts",
      "lint:fix": "eslint src/**/*.ts --fix",
      "format": "prettier --write \"src/**/*.ts\"",
      "prepare": "npm run build"
    },
    "keywords": [
      "api",
      "gateway",
      "microservices",
      "health"
    ],
    "author": "Holistic Health OS Team",
    "license": "MIT",
    "dependencies": {
      "@types/express": "^4.17.13",
      "@types/node": "^22.14.0",
      "axios": "^1.6.8",
      "cors": "^2.8.5",
      "dotenv": "^16.4.7",
      "express": "^5.1.0",
      "express-rate-limit": "^7.5.0",
      "helmet": "^8.1.0",
      "http-proxy-middleware": "^2.0.6",
      "joi": "^17.13.3",
      "jsonwebtoken": "^9.0.2",
      "morgan": "^1.10.0",
      "swagger-jsdoc": "^6.2.8",
      "swagger-ui-express": "^5.0.0",
      "winston": "^3.13.0"
    },
    "devDependencies": {
      "@types/cors": "^2.8.17",
      "@types/http-proxy-middleware": "^1.0.0",
      "@types/morgan": "^1.9.9",
      "@types/swagger-jsdoc": "^6.0.4",
      "@types/swagger-ui-express": "^4.1.6",
      "@typescript-eslint/eslint-plugin": "^7.14.0",
      "@typescript-eslint/parser": "^7.14.0",
      "eslint": "^8.57.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-prettier": "^5.1.3",
      "nodemon": "^3.1.9",
      "prettier": "^3.3.0",
      "rimraf": "^5.0.5",
      "ts-node": "^10.9.2",
      "typescript": "^5.8.3"
    },
    "engines": {
      "node": ">=18.0.0"
    }
  }