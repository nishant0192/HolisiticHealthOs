{
    "name": "user-service",
    "version": "1.0.0",
    "description": "User service for Holistic Health OS",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc -p tsconfig.json",
        "clean": "rimraf dist",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "prepare": "npm run build"
    },
    "keywords": [
        "user",
        "profile",
        "health",
        "microservice"
    ],
    "author": "Holistic Health OS Team",
    "license": "MIT",
    "dependencies": {
        "@types/express": "^5.0.1",
        "@types/node": "^22.14.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^5.1.0",
        "helmet": "^8.1.0",
        "joi": "^17.13.3",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "pg": "^8.14.1",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "uuid": "^11.1.0",
        "winston": "^3.13.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/morgan": "^1.9.9",
        "@types/pg": "^8.11.11",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^7.14.0",
        "@typescript-eslint/parser": "^7.14.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "nodemon": "^3.1.9",
        "prettier": "^3.3.0",
        "rimraf": "^5.0.5",
        "typescript": "^5.8.3"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
